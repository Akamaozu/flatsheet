{
  "name": "flatsheet-editor",
  "description": "a multi-user, realtime editor for tabular data",
  "version": "0.0.0",
  "repository": {
    "url": "git://github.com/flatsheet/editor.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "tape test.js",
    "bundle-css": "rework-npm style.css | myth | cleancss -o public/app.css",
    "watch-css": "gazer -p 'style.css' 'rework-npm style.css | myth > public/app.css'",
    "watch-js": "watchify -d browser.js -o public/app.js -t brfs",
    "bundle-js": "browserify -t brfs browser.js -o public/app.js",
    "bundle": "npm run bundle-js & npm run bundle-css",
    "watch": "npm run watch-js & npm run watch-css",
    "start": "node server.js"
  },
  "dependencies": {
    "JSONStream": "^0.9.0",
    "accountdown": "^1.0.0",
    "accountdown-basic": "^1.0.0",
    "body": "^4.5.0",
    "comma-separated-values": "^3.3.0",
    "component-closest": "^0.1.4",
    "component-delegate": "^0.2.3",
    "dom-event": "0.0.0",
    "element-class": "^0.1.1",
    "handlebars": "~2.0",
    "handlebars-layouts": "^0.3.3",
    "http-methods": "^0.1.0",
    "jsonpretty": "0.0.1",
    "level": "^0.18.0",
    "level-session": "^0.3.1",
    "level-sublevel": "^6.2.0",
    "levelup": "^0.18.6",
    "normalize-css": "^2.3.1",
    "random-color": "0.0.0",
    "response": "git://github.com/mikeal/response",
    "routes": "^1.2.0",
    "routes-router": "^4.1.0",
    "sendgrid": "^1.2.0",
    "shoe": "0.0.15",
    "st": "^0.5.1",
    "table-editor": "^0.4.3",
    "through": "^2.3.4",
    "uuid": "^1.4.1",
    "websocket-stream": "^0.6.0",
    "ws": "^0.4.32"
  },
  "devDependencies": {
    "brfs": "^1.1.1",
    "browserify": "^4.1.6",
    "clean-css": "^2.2.8",
    "gazer": "0.0.3",
    "minimist": "^1.1.0",
    "myth": "^1.1.0",
    "request": "^2.40.0",
    "rework-npm-cli": "^0.1.1",
    "tape": "^2.14.0",
    "through2": "^0.6.1",
    "uglify-js": "^2.4.13",
    "watchify": "^0.10.1"
  }
}
